<div id="response-wrapper" class="category-wrapper">
    <div class="category-grid">
        {% for search_result in search_array %}
            <a href="#" class="product-tile">
                <div class="product-shot">
                    
					{% if search_result.url %}
									<img src="{{ search_result.url }}" />
								{% else %}
									<img src="http://brancottvanillascfanqpukk.devcloud.acquia-sites.com/sites/default/files/styles/medium/public/2017-02/Brancott-Estate_Sauv_Blanc_2.png" />
								{% endif %}
                    <div class="product-bg"></div>
                </div>
                <h3>{{search_result.title }}</h3>
                <p>{{search_result.range }}</p>
            </a>
        {% endfor %}
    </div>
</div>