	<div class="category-grid">
		{% for search_result in search_array %}
		<a href="#" class="product-tile">
			<div class="product-shot">
				{% if search_result.url %}
				<img src="{{ search_result.url }}" class="primary" /><img src="{{ search_result.url }}" class="secondary" />
				{% else %}
				<img src="http://brancottvanillaxjhdrqa7rm.devcloud.acquia-sites.com/sites/default/files/2017-03/bottle-not-final-showcase-series.png" class="primary" /><img src="http://brancottvanillaxjhdrqa7rm.devcloud.acquia-sites.com/sites/default/files/2017-03/bottle-not-final-showcase-series.png" class="secondary" />
				{% endif %}
				<div class="product-bg"></div>
			</div>
			<h3>{{search_result.title }}</h3>
			<p>{{search_result.range }}</p>
		</a>
		{% endfor %}
	</div>