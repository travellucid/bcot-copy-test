<!-- carousel -->
		<div class="carousel-background">
			
		</div>
		<div id="carousel-our-wines" class="carousel slide container-fluid" data-ride="carousel"  data-interval="false">
			<!-- Indicators -->
			<ol class="carousel-indicators">
				{% for i in 0..5 %}
		      {% if loop.first %}
		      <li data-target="#carousel-our-wines" data-slide-to="{{ i }}" class="active"></li>
		    {% elseif not loop.last %}
		      <li data-target="#carousel-our-wines" data-slide-to="{{ i }}"></li>
		    {% endif %}
		    {% endfor %}
			</ol>
			<!-- Wrapper for slides -->
			<div class="carousel-inner " role="listbox">
			   {# {% for i in 0..count %} #}
			   {% for i in 0..4 %}
                <div class="item">
					<img src="http://lighteningbrancottrpszypctoa.devcloud.acquia-sites.com/sites/default/files/styles/vineyard_style/public/2017-01/Brancott-Estate_Sauv_Blanc_X2.png" alt="...">
					
					<!-- carousal description -->
						<div class="carousal-desc-wrapper">
							<h2 class="header-d carousal-header">{{ names[i] }}</h2>
							<p class="carousal-description">Bright, Crisp & clearly unmistakable.</p>
							<a href="" class="cta light">VIEW RANGE</a>
						</div>
				</div>
               {% endfor %}
								
				
			</div>
			<!-- Controls -->
			<div class="carousel-control-wrapper hidden-sm hidden-xs">
				<a class="left carousel-control" href="#carousel-our-wines" role="button" data-slide="prev">
				<span class="prev-carousal" aria-hidden="true"><span class="glyphicon glyphicon-left"></span></span>
				<span class="sr-only">Previous</span>
				</a>
				<a class="right carousel-control" href="#carousel-our-wines" role="button" data-slide="next">
				<span class="next-carousal" aria-hidden="true"><span class="glyphicon glyphicon-right" style=""></span></span>
				
				<span class="sr-only">Next</span>
				</a>
			</div>
		</div>