<section id="find-us-component">
<div class="map_bound" data-right_bound="{{ content.field_bottom_right_bound.0 }}" data-left_bound="{{ content.field_left_top_bound.0 }}" id="map-overlay">
    <h2>{{ content.field_find_us_title.0 }} </h2>
    <h3 class="header_f">{{ content.field_address_title.0 }} </h3>
    <p>{{ content.field_address_line_1.0 }} </p>
    <p>{{ content.field_add.0 }} </p>
    <h3 class="header_f">{{ content.field_contact_us_title.0 }} </h3>
    <p>{{ content.field_contact_us_email_label.0 }}<a href="mailto:{{ content.field_contact_us_email.0 }}">{{ content.field_contact_us_email.0 }} </a></p>
    <p>{{ content.field_call_us_on.0 }} <a href="tel:{{ content.field_contact_us_phone.0 }}">{{ content.field_contact_us_phone.0 }} </a></p>
    <h3 class="header_f">{{ content.field_opening_hours_title.0 }} </h3>
    <p>{{ content.field_opening_hours_line_1.0 }} </p>
    <p>{{ content.field_opening_hours_line_2.0 }} </p>
    <p>{{ content.field_opening_hours_line_3.0 }} </p>
</div>
<div class="map" id ="google-map-component">
    <div>{{ content.field_address.0 }} </div>
</div>
</section>