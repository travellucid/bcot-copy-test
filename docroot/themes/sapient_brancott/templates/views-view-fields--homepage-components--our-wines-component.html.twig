<div id="our-wines">
	<div class="our-wines-background-container">
		<div class="our-wines-wrapper col-md-8 col-sm-12">
			<h1 class="header-d our-wine-header">{{ fields.field_our_wine_small_heading.content }}</h1>
			<h2 class="header-d wines-sub-heading">{{ fields.field_our_wine_main_headline.content }}</h2>
			<p>{{ fields.field_our_wine_description.content }}</p>
			<a href="{{ fields.field_our_wine_cta.content }}" class="cta dark view-wines-btn">{{ fields.field_our_wine_cta_1.content }}</a>
		</div>
		<div class="vineyard col-sm-12">
			<span class="vineyard-desc">{{ fields.field_our_wine_photo_title.content|upper }}</span>
			<img class="vineyard-pic" src="{{ fields.field_background_image.content }}" alt="" data-mobile-src = "{{ fields.field_background_image_1.content }}" data-desktop-src = "{{ fields.field_background_image.content }}">
		</div>
		<!-- carousel -->
		{{ our_wine_carousel_block }}
	</div>
	<div class="our-wines-youtube-video">
		<iframe src="https://www.youtube.com/embed/{{ fields.field_youtube_video.content }}?rel=0" class ="video">
		</iframe>
		<p>{{ fields.field_youtube_video_title.content|upper }}</p>
	</div>
</div>
