<div id="follow-us" class="cfix container-fluid">
    <div id="gallery">
        {{ fields.view.content }}
    </div>
    <a href="{{ fields.field_social_feed_instagram_link.content }}" class="icon-Instagram">
        <div id="content">
            <div class="follow-us-text">
                <h3 class="hashtag header_f">{{ fields.field_instagram_hashtag.content |capitalize }}</h3>
                <h2 class="follow-text header_c">{{ fields.field_instagram_tagline.content }}</h2>

            </div>
        </div>
    </a>
</div>